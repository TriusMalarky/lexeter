        for i in room.loot:
            list.append(i)
        print(""What do you want to inspect?"")
        for i in list:
            print("" - ""+i)
        def invLoop(world):
            choice = input("": "")
            if choice in world.player.inventory:
                item = getattr(world.item, choice)
                print(random.choice(item.descriptions))
            else:
                invLoop(world)
        def loop(self,world):
            choice = input("": "")
