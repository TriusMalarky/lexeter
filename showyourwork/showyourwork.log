    def playeraction(self):
        resp = input("": "")
        found = False
        for item in self.array:
            if resp == self.array[item]:
                found=True
                exec(""self."" + item + ""()"")
        if found == False:
            print(""That's not a possible action. Try again, or use '.h' or 'help' to see the help screen."")
