class playerAct(core):
    def __init__(self,player,world):
        self.internalID = 'player-action-library'
        self.player=player;self.world=world
        self.array = {
            'moveTo': '.m',
            'moveAlias':'move',
            'help':'.h',
            'helpAlias': 'help',
            'inspect':'.i',
            'inspectAlias':'inspect',
            'wait':'.w',
            'waitAlias':'wait',
            'scream':'scream',
